<!-- 集装箱 -->  
  <ThingDef Name="LunaAddShippingContainer" ParentName="LWM_DeepStorage" Abstract="True">
		<designatorDropdown>LunaAddSimplyCrateShippingContainer</designatorDropdown>
    <description>By compressing space and objects, this shipping container is able to store incredibly large quantities of items!</description>
    <size>(4,3)</size>
    <statBases>
      <Beauty>0.7</Beauty>
      <MaxHitPoints>2200</MaxHitPoints>
      <WorkToBuild>6000</WorkToBuild>
      <Flammability>0.1</Flammability>
      <Mass>1000</Mass>
      <StyleDominance MayRequire="Ludeon.RimWorld.Ideology">5</StyleDominance>
    </statBases>
     <costList>
      <Steel>2000</Steel>
	    <Plasteel>150</Plasteel>
	    <ComponentIndustrial>20</ComponentIndustrial>
    </costList>
    <building>
      <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
      <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
      <defaultStorageSettings>
        <priority>Low</priority>
        <filter>
        </filter>
      </defaultStorageSettings>
    </building>
    <comps>
      <li Class="LWM.DeepStorage.Properties" >
        <minNumberStacks>20</minNumberStacks>
        <maxNumberStacks>100</maxNumberStacks><!-- In-game 1200 -->
        <timeStoringTakes>250</timeStoringTakes>
        <showContents>false</showContents>
		<overlayType>None</overlayType>
      </li>
    </comps>
	 <researchPrerequisites>
      <li>LunaCSS_CompactShippingContainer</li>
    </researchPrerequisites>
  </ThingDef>
 
