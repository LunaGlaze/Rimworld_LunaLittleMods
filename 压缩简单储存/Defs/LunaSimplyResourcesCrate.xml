<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <DesignatorDropdownGroupDef>
  	<defName>LunaAddSimplyResourcesCrate</defName>
  	<label>Simply Resources Crate</label>
  </DesignatorDropdownGroupDef>
  
  <!-- ==================== ResourcesCrate Base ==================== -->

  <ThingDef Name="LunaAddResourcesCrateBase" ParentName="LWM_DeepStorage" Abstract="True">
    <defaultPlacingRot>South</defaultPlacingRot>
    <statBases>
      <Beauty>0.7</Beauty>
    </statBases>
    <building>
      <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
      <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
      <defaultStorageSettings>
        <priority>Low</priority>
      </defaultStorageSettings>
    </building>	
		<researchPrerequisites>
			<li>LunaCSS_CompactResourcesCrate</li>
		</researchPrerequisites>
    <comps>
      <li Class="LWM.DeepStorage.Properties" >
        <minNumberStacks>1</minNumberStacks>
        <maxNumberStacks>50</maxNumberStacks> <!-- In-game 50 -->
        <timeStoringTakes>120</timeStoringTakes>
        <showContents>false</showContents>
		<overlayType>None</overlayType>
      </li>
    </comps>
  </ThingDef>

 	<!-- ==================== ResourcesCrate1 ==================== -->

  <ThingDef ParentName="LunaAddResourcesCrateBase" >
    <defName>Luna_Add1ResourcesCrate</defName>
    <label>WoodLog CompactResourcesCrate</label>
    <description>Can Storage up to 50 Stack of WoodLog</description>
		<designatorDropdown>LunaAddSimplyResourcesCrate</designatorDropdown>
    <graphicData>
      <texPath>Things/ResourcesCrate/ResourcesCrate1</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(1.3,1.3)</drawSize>
      <drawRotated>false</drawRotated>
    </graphicData>
    <uiIconPath>Things/ResourcesCrate/ResourcesCrate1</uiIconPath>
    <castEdgeShadows>true</castEdgeShadows>
    <size>(1,1)</size>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <WorkToBuild>950</WorkToBuild>
      <Flammability>1</Flammability>
      <Mass>15</Mass>
    </statBases>
    <costList>
      <WoodLog>90</WoodLog>
    </costList>
    <building>
      <fixedStorageSettings>
        <filter>
        <thingDefs>
          <li>WoodLog</li>
        </thingDefs>
        </filter>
      </fixedStorageSettings>
      <defaultStorageSettings>
        <filter>
        <thingDefs>
          <li>WoodLog</li>
        </thingDefs>
        </filter>
      </defaultStorageSettings>
    </building>		
  </ThingDef>
 
