                    <drawRotated>false</drawRotated>
                    <shaderType>CutoutComplex</shaderType>
                </graphicData>
            </li>
        </graphics>
        <stuffFilter>
            <stuffCategoriesToAllow>
                <li>Metallic</li>
                <li>LCRC_CanMakeResourcesCrate</li>
            </stuffCategoriesToAllow>
            <disallowedThingDefs>
                <li>WoodLog</li>
                <li>Steel</li>
                <li>Plasteel</li>
                <li>Jade</li>
                <li>Gold</li>
                <li>Silver</li>
                <li>Uranium</li>
                <li MayRequire="Ludeon.RimWorld.Anomaly">Bioferrite</li>
                <li MayRequire="Ludeon.RimWorld.Odyssey">Obsidian</li>
                <li MayRequire="lunaglaze.Anomaly.BiotechnologyReforgedFlesh">BRF_ScarletFerrite</li>
                <li MayRequire="lunaglaze.Anomaly.BiotechnologyReforgedFlesh">BRF_CrimsonStem</li>
