<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <StuffCategoryDef>
    <defName>LCRC_CanMakeResourcesCrate</defName>
    <label>can make resources crate</label>
    <destroySoundSmall>BuildingDestroyed_Stone_Small</destroySoundSmall>
    <destroySoundMedium>BuildingDestroyed_Stone_Medium</destroySoundMedium>
    <destroySoundLarge>BuildingDestroyed_Stone_Big</destroySoundLarge>
  </StuffCategoryDef>
  
  <!-- ==================== ResourcesCrate Base ==================== -->

  <ThingDef Name="LCRC_AddResourcesCrateBase" ParentName="AdaptiveStorageBase" Abstract="True">
    <statBases>
      <Beauty>0.7</Beauty>
      <MaxHitPoints>320</MaxHitPoints>
      <WorkToBuild>2500</WorkToBuild>
      <Flammability>1</Flammability>
      <Mass>7.5</Mass>
    </statBases>
    <castEdgeShadows>true</castEdgeShadows>
    <size>(1,1)</size>
    <costList>
      <WoodLog>74</WoodLog>
    </costList>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>LCRC_CanMakeResourcesCrate</li>
    </stuffCategories>
    <costStuffCount>1</costStuffCount>
    <rotatable>false</rotatable>
    <fillPercent>0.8</fillPercent>
    <building>
      <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
      <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
      <maxItemsInCell>45</maxItemsInCell>
      <fixedStorageSettings>
        <filter>
            <categories>
                <li>Root</li>
            </categories>
        </filter>
      </fixedStorageSettings>
      <defaultStorageSettings>
        <filter>
            <categories>
                <li>Root</li>
            </categories>
        </filter>
      </defaultStorageSettings>
    </building>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
        <li>BuildingsFurniture</li>
    </thingCategories>
    <!--researchPrerequisites>
        <li>LunaCSS_CompactResourcesCrate</li>
    </researchPrerequisites-->
  </ThingDef>

    <!-- ==================== ResourcesCrate Wooden ==================== -->

  <ThingDef ParentName="LCRC_AddResourcesCrateBase" Name="LCRC_WoodenResourcesCrateBase">
    <defName>LCRC_WoodenResourcesCrate</defName>
    <label>Compact Resources Crate</label>
    <description>Can Storage up to 45 Stack of Resources</description>
    <graphicData>
      <texPath>Luna_CRC/ResourcesCrate/WoodenResourcesCrateType_other</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(1.3,1.3)</drawSize>
      <drawRotated>false</drawRotated>
    </graphicData>
    <uiIconPath>Luna_CRC/ResourcesCrate/WoodenResourcesCrateType_other</uiIconPath>
    <uiIconScale>1</uiIconScale>
    <building>
      <paintable>true</paintable>
    </building>
        <researchPrerequisites>
            <li>LunaCRC_CompactResourcesCrate</li>
        </researchPrerequisites>
      <modExtensions>
          <li Class="AdaptiveStorage.Extension">
        <lockStorageSettingsToStuff>true</lockStorageSettingsToStuff>
        <labelFormat>StuffAsNoun</labelFormat>
      </li>
    </modExtensions>
  </ThingDef>

  <DesignatorDropdownGroupDef>
    <defName>LCRC_LunaAddWoodenResourcesCrate</defName>
    <label>Compact Resources Crate</label>
  </DesignatorDropdownGroupDef>

