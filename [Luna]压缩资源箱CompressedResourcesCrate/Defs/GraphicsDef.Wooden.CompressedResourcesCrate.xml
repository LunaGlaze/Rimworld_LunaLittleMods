<!--其他-->
    <AdaptiveStorage.GraphicsDef>
        <defName>LCRC_WoodenResourcesCrate_other</defName>
        <targetDef>LCRC_WoodenResourcesCrate</targetDef>
        <showContainedItems>false</showContainedItems>
        <graphics>
            <li>
                <graphicData>
                    <texPath>Luna_CRC/ResourcesCrate/WoodenResourcesCrateType_other</texPath>
