                    <drawSize>(1.3,1.3)</drawSize>
                    <drawRotated>false</drawRotated>
                    <shaderType>CutoutComplex</shaderType>
                </graphicData>
            </li>
        </graphics>
        <stuffFilter>
            <stuffCategoriesToAllow>
                <li>Metallic</li>
                <li>LCRC_CanMakeResourcesCrate</li>
            </stuffCategoriesToAllow>
            <disallowedThingDefs>
                <li>WoodLog</li>
                <li>Steel</li>
                <li>Plasteel</li>
                <li>Jade</li>
                <li>Gold</li>
                <li>Silver</li>
                <li>Uranium</li>
                <li MayRequire="Ludeon.RimWorld.Anomaly">Bioferrite</li>
                <li MayRequire="Ludeon.RimWorld.Odyssey">Obsidian</li>
                <li MayRequire="lunaglaze.Anomaly.BiotechnologyReforgedFlesh">BRF_ScarletFerrite</li>
                <li MayRequire="lunaglaze.Anomaly.BiotechnologyReforgedFlesh">BRF_CrimsonStem</li>
                <li MayRequire="lunaglaze.Anomaly.BiotechnologyReforgedFlesh">BRF_RecastFleshCube</li>
                <li MayRequire="lunaglaze.Anomaly.BiotechnologyReforgedFlesh,Ludeon.RimWorld.Odyssey">BRF_Bloodsidian</li>
                <li MayRequire="ninedaylongbow.ChineseComprehensiveExpansion">HX_CivilCeramics</li>
                <li MayRequire="ninedaylongbow.ChineseComprehensiveExpansion">HX_MilitaryCeramics</li>
                <li MayRequire="lunaglaze.Anomaly.BiotechnologyReforgedFlesh,ninedaylongbow.ChineseComprehensiveExpansion">BRF_BloodSakuraCeramics</li>
                <li MayRequire="sarg.alphabiomes">AB_AlcyoniteChunk</li>
                <li MayRequire="sarg.alphaanimals">AA_SkySteel</li>
                <li MayRequire="tw.tangsbiome.rainbowforest">TWRL_Resource_PinkCrystal</li>
            </disallowedThingDefs>
        </stuffFilter>
